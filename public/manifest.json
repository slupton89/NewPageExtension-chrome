{
  "short_name": "ZenTab",
  "name": "ZenTab",
  "description": "A fresh new take on new tabs",
  "version": "0",
  "manifest_version": 2,
  "permissions": [
    "topSites",
    "identity",
    "geolocation",
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "chrome_url_overrides" : {
  "newtab": "index.html"
  }
}
